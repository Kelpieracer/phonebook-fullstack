{
  "name": "phonebookback",
  "version": "3.8.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "cd client && start",
    "start": "ts-node src/server.ts",
    "watch:debug": "node_modules/.bin/nodemon --watch ./src --inspect=5858 -e ts,tsx --exec node -r ts-node/register index.ts",
    "server-doc": "typedoc --out client/build/server-doc ./src --plugin none --esModuleInterop",
    "build-server": "tsc -p . && npm run-script server-doc",
    "build-client": "cd client && npm run build && npm run-script client-doc",
    "build": "npm run-script build-client && npm run-script build-server",
    "tsc": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "script": "cd src/scripts && ts-node"
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "exec": "node_modules/.bin/ts-node index.ts  -e ts",
    "ext": "js, json, ts, proto"
  },
  "author": "Matti Luukkainen",
  "license": "MIT",
  "dependencies": {
    "@types/mongoose": "^5.7.16",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "mongoose": "^5.9.13"
  },
  "eslintConfig": {
    "extends": "part3-1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "axios": "^0.19.2",
    "eslint": "^6.8.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  }
}
