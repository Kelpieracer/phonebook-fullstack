{
  "name": "notebackend",
  "version": "0.0.1",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "node_modules/.bin/nodemon --watch . -e ts --exec node_modules/.bin/ts-node index.ts",
    "start": "node_modules/.bin/ts-node index.ts",
    "watch:debug": "node_modules/.bin/nodemon --inspect-brk=5858 -e ts,tsx --exec node -r ts-node/register ./index.ts",
    "watch:debug2": "node_modules/.bin/nodemon --inspect=5858 -e ts,tsx --exec node -r ts-node/register ./index.ts",
    "build": "tsc --project .",
    "tsc": "tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "nodemonConfig": {
    "watch": [
      "./"
    ],
    "exec": "node_modules/.bin/ts-node index.ts  -e ts",
    "ext": "js, json, ts, proto"
  },
  "author": "Matti Luukkainen",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "eslint": "^6.8.0",
    "express": "^4.16.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "part3-1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "nodemon": "^1.19.4"
  }
}
